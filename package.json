{
  "name": "@pelusa/create-node-ts-boilerplate",
  "version": "0.0.11",
  "publishConfig": {
    "public": true
  },
  "bin": {
    "create-node-ts-boilerplate": "bin/createNodeTsApp.js"
  },
  "description": "NodeJS Typescript Boilerplate",
  "main": "build/index.js",
  "scripts": {
    "prepublishOnly": "npm run eslint && npm run build",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts --no-stdin",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "eslint": "eslint . --ext .js,.ts",
    "eslint:fix": "eslint . --ext .js,.ts --fix"
  },
  "keywords": [
    "node",
    "typescript",
    "boilerplate"
  ],
  "author": "Tom√°s Catena",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.2.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1"
  }
}
